{{! nq }}
<img src="{{img_url data.main_nav.logo_url}}" alt="{{data.main_nav.logo_alt}}" class="logo">

<nav class="nav swiper-container {{#if data.main_nav.optional_classes}}{{data.main_nav.optional_classes}}{{/if}}"
     role="navigation">
    <div class="main__container swiper-wrapper">
        {{#if data.isdevelop}}
            <div class="swiper-slide nav__items">

                {{#each data.main_nav.navpoints}}
                    <div class="nav__item {{#ifCond this.navpoint_name '=='
                                                    @local.pageID}} nav__item--active{{/ifCond}}">
                        <a href="{{this.navpoint_dev_url}}">{{this.navpoint_title}}</a>
                    </div>
                {{/each}}
            </div>


            <div class="swiper-slide">
                {{#each data.main_nav.swipernavpoints}}
                    <div class="nav__item {{#ifCond this.navpoint_name '==' @local.pageID}} nav__item--active{{/ifCond}}">
                        <a href="{{this.navpoint_dev_url}}">{{this.navpoint_title}}</a>
                    </div>
                {{/each}}
            </div>

        {{else}}

            <div class="swiper-slide nav__items">

                {{#each data.main_nav.navpoints}}
                    <div class="nav__item {{#ifCond this.navpoint_name '=='
                                                    @local.pageID}} nav__item--active{{/ifCond}}">
                        <a href="{{this.navpoint_url}}">{{this.navpoint_title}}</a>
                    </div>
                {{/each}}
            </div>

            <div class="swiper-slide">
                {{#each data.main_nav.swipernavpoints}}
                    <div class="nav__item {{#ifCond this.navpoint_name '=='
                                                    @local.pageID}} nav__item--active{{/ifCond}}">
                        <a href="{{this.navpoint_url}}">{{this.navpoint_title}}</a>
                    </div>
                {{/each}}
            </div>

        {{/if}}


    </div>
</nav>
